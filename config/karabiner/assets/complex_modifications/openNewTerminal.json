{
"title": "Terminal",
"rules": [
            {
            "description": "Open a new terminal window",
            "manipulators": [
                {
                  "description": "Open a new terminal window",
                  "from": {
                    "key_code": "t",
                    "modifiers": {
                      "mandatory": [
                        "command",
                        "shift"

                      ]
                    }
                  },
                  "to": [
                    {
                      "shell_command": "open -aterminal -n"
                    }
                  ],
                  "type": "basic"
                }
            ]
        }
    ]
}
